	Name "DSP LoopbackBT Mic"

	EnableSequence
		'Voice Stub Tx Mixer AUX_PCM_TX_Voice':1:1
		'SLIMBUS_1_RX Mixer Voice Stub':1:1
		'Voice Stub Tx Mixer SLIM_1_TX':1:1
		'AUX_PCM_RX_Voice Mixer Voice Stub':1:1
		'SLIMBUS_1_RX Port Mixer AUX_PCM_TX_Voice':1:1
		'AUX_PCM_RX Port Mixer SLIM_1_TX':1:1
		'SLIM_3_RX Channels':0:One
		'SLIMBUS_3_RX_Voice Mixer Voice Stub':1:1
		'SLIMBUS_3_RX Port Mixer AUX_PCM_RX':1:1
