SectionUseCase."Loopback" {
Comment "PCM Loopback use case"
SectionVerb
	Name "Loopback"

	EnableSequence
		'STUB_RX Mixer Voice Stub':1:1
		'Voice Stub Tx Mixer STUB_TX_HL':1:1	
	EndSequence

	DisableSequence
		'STUB_RX Mixer Voice Stub':1:0
		'Voice Stub Tx Mixer STUB_TX_HL':1:0	
	EndSequence

	# ALSA PCMs
	PlaybackPCM 12
	CapturePCM 12

EndSection
}
SectionUseCase."DSP Loopback" {
Comment "Packet Loopback use case"
SectionVerb
	Name "DSP Loopback"

	EnableSequence
		'STUB_RX Mixer Voice Stub':1:1
		'Voice Stub Tx Mixer STUB_TX_HL':1:1
	EndSequence

	DisableSequence
		'STUB_RX Mixer Voice Stub':1:0
		'Voice Stub Tx Mixer STUB_TX_HL':1:0
	EndSequence

	# ALSA PCMs
	PlaybackPCM 12
	CapturePCM 12

EndSection

SectionVerb
	Name "DSP LoopbackBT Headset"

	EnableSequence
	EndSequence

	DisableSequence
		'Voice Stub Tx Mixer AUX_PCM_TX_Voice':1:0
		'SLIMBUS_1_RX Mixer Voice Stub':1:0
		'Voice Stub Tx Mixer SLIM_1_TX':1:0
		'AUX_PCM_RX_Voice Mixer Voice Stub':1:0
		'SLIMBUS_1_RX Port Mixer AUX_PCM_TX_Voice':1:0
		'AUX_PCM_RX Port Mixer SLIM_1_TX':1:0
		'SLIMBUS_3_RX_Voice Mixer Voice Stub':1:0
		'SLIMBUS_3_RX Port Mixer AUX_PCM_RX':1:0
	EndSequence

	# ALSA PCMs
	PlaybackPCM 12
	CapturePCM 12

EndSection

